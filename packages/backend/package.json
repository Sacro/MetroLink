{
  "name": "metrolink-backend",
  "version": "0.0.1",
  "description": "Metrolink Backend",
  "author": "Ben Woodward",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "cross-env NODE_ENV=development nodemon",
    "start:debug": "cross-env NODE_ENV=development nodemon --config nodemon-debug.json",
    "prestart:prod": "tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:mutate": "stryker run",
    "typeorm": "ts-node ./node_modules/.bin/typeorm",
    "webpack": "webpack --config webpack.config.js"
  },
  "engines": {
    "node": "10.x"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.1",
    "@nestjs/core": "^6.0.1",
    "@nestjs/graphql": "^6.0.1",
    "@nestjs/typeorm": "^6.0.0",
    "apollo-server-express": "^2.1.0",
    "class-transformer": "^0.2.0",
    "class-validator": "^0.9.1",
    "dotenv": "^7.0.0",
    "graphql": "^14.0.2",
    "graphql-redis-subscriptions": "^2.0.0",
    "graphql-tools": "^4.0.3",
    "joi": "^14.0.1",
    "luxon": "^1.5.0",
    "nest-schedule": "^0.4.3",
    "pg": "^7.5.0",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.2.2",
    "stompit": "^0.26.0",
    "stompjs": "^2.3.3",
    "type-graphql": "^0.17.0",
    "typeorm": "^0.2.8",
    "typescript": "^3.0.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.1",
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/ioredis": "^4.0.3",
    "@types/jest": "^24.0.11",
    "@types/joi": "^14.0.0",
    "@types/luxon": "^1.4.0",
    "@types/node": "^11.11.3",
    "@types/stompit": "^0.26.0",
    "@types/stompjs": "^2.3.4",
    "@types/supertest": "^2.0.5",
    "cross-env": "^5.2.0",
    "jest": "^24.5.0",
    "jest-extended": "^0.11.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "@stryker-mutator/core": "^1.1.0",
    "@stryker-mutator/html-reporter": "^1.1.0",
    "@stryker-mutator/jest-runner": "^1.1.0",
    "@stryker-mutator/typescript": "^1.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.0",
    "ts-loader": "^5.2.2",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.14.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
